<!--pages/home/childComps/recommend/myRecommend.wxml-->
<view class="recommend">
  <block wx:for="{{list}}" wx:key="index">
    <view class="recommend-item">
      <image class="image" src="{{item.image}}" bind:load="imageLoad" />
      <view class="text">{{item.title}}</view>
    </view>
  </block>
</view>
